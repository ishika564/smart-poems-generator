en'],\n ['seem',\n  'all',\n  'on',\n  'fire',\n  'that',\n  'chapel',\n  'proud',\n  'where',\n  'roslin',\n  'chiefs',\n  'uncoffin',\n  'lie',\n  'each',\n  'baron',\n  'for',\n  'sable',\n  'shroud',\n  'sheath',\n  'in',\n  'his',\n  'iron',\n  'panoply'],\n ['seem',\n  'all',\n  'on',\n  'fire',\n  'within',\n  'around',\n  'deep',\n  'sacristy',\n  'and',\n  'altar',\n  'pale',\n  'shone',\n  'every',\n  'pillar',\n  'foliage',\n  'bound',\n  'and',\n  'glimmer',\n  'all',\n  'the',\n  'dead',\n  'men',\n  'mail'],\n ['blaz',\n  'battlement',\n  'and',\n  'pinnet',\n  'high',\n  'blaz',\n  'every',\n  'rose',\n  'carved',\n  'buttress',\n  'fair',\n  'so',\n  'still',\n  'they',\n  'blaze',\n  'when',\n  'fate',\n  'is',\n  'nigh',\n  'the',\n  'lordly',\n  'line',\n  'of',\n  'high',\n  'st',\n  'clair'],\n ['there',\n  'are',\n  'twenty',\n  'of',\n  'roslin',\n  'barons',\n  'bold',\n  'lie',\n  'buried',\n  'within',\n  'that',\n  'proud',\n  'chapelle',\n  'each',\n  'one',\n  'the',\n  'holy',\n  'vault',\n  'doth',\n  'hold',\n  'but',\n  'the',\n  'sea',\n  'holds',\n  'lovely',\n  'rosabelle'],\n ['and',\n  'each',\n  'st',\n  'clair',\n  'was',\n  'buried',\n  'there',\n  'with',\n  'candle',\n  'with',\n  'book',\n  'and',\n  'with',\n  'knell',\n  'but',\n  'the',\n  'sea',\n  'caves',\n  'rung',\n  'and',\n  'the',\n  'wild',\n  'winds',\n  'sung',\n  'the',\n  'dirge',\n  'of',\n  'lovely',\n  'rosabelle'],\n ['one',\n  'morning',\n  'of',\n  'the',\n  'first',\n  'sad',\n  'fall',\n  'poor',\n  'adam',\n  'and',\n  'his',\n  'bride',\n  'sat',\n  'in',\n  'the',\n  'shade',\n  'of',\n  'eden',\n  'wall',\n  'but',\n  'on',\n  'the',\n  'outer',\n  'side'],\n ['she',\n  'blushing',\n  'in',\n  'her',\n  'fig',\n  'leaf',\n  'suit',\n  'for',\n  'the',\n  'chaste',\n  'garb',\n  'of',\n  'old',\n  'he',\n  'sighing',\n  'er',\n  'his',\n  'bitter',\n  'fruit',\n  'for',\n  'eden',\n  'drupes',\n  'of',\n  'gold'],\n ['behind',\n  'them',\n  'smiling',\n  'in',\n  'the',\n  'morn',\n  'their',\n  'forfeit',\n  'garden',\n  'lay',\n  'before',\n  'them',\n  'wild',\n  'with',\n  'rock',\n  'and',\n  'thorn',\n  'the',\n  'desert',\n  'stretched',\n  'away'],\n ['they',\n  'heard',\n  'the',\n  'air',\n  'above',\n  'them',\n  'fanned',\n  'light',\n  'step',\n  'on',\n  'the',\n  'sward',\n  'and',\n  'lo'],\n ['they',\n  'saw',\n  'before',\n  'them',\n  'stand',\n  'the',\n  'angel',\n  'of',\n  'the',\n  'lord'],\n ['arise',\n  'he',\n  'said',\n  'why',\n  'look',\n  'behind',\n  'when',\n  'hope',\n  'is',\n  'all',\n  'before',\n  'and',\n  'patient',\n  'hand',\n  'and',\n  'willing',\n  'mind',\n  'your',\n  'loss',\n  'may',\n  'yet',\n  'restore'],\n ['leave',\n  'with',\n  'you',\n  'spell',\n  'whose',\n  'power',\n  'can',\n  'make',\n  'the',\n  'desert',\n  'glad',\n  'and',\n  'call',\n  'around',\n  'you',\n  'fruit',\n  'and',\n  'flower',\n  'as',\n  'fair',\n  'as',\n  'eden',\n  'had'],\n ['clothe',\n  'your',\n  'hands',\n  'with',\n  'power',\n  'to',\n  'lift',\n  'the',\n  'curse',\n  'from',\n  'off',\n  'your',\n  'soil',\n  'your',\n  'very',\n  'doom',\n  'shall',\n  'seem',\n  'gift',\n  'your',\n  'loss',\n  'gain',\n  'through',\n  'toil'],\n ['go',\n  'cheerful',\n  'as',\n  'yon',\n  'humming',\n  'bees',\n  'to',\n  'labor',\n  'as',\n  'to',\n  'play'],\n ['white',\n  'glimmering',\n  'over',\n  'eden',\n  'trees',\n  'the',\n  'angel',\n  'passed',\n  'away'],\n ['the',\n  'pilgrims',\n  'of',\n  'the',\n  'world',\n  'went',\n  'forth',\n  'obedient',\n  'to',\n  'the',\n  'word',\n  'and',\n  'found',\n  'where',\n  'er',\n  'they',\n  'tilled',\n  'the',\n  'earth',\n  'garden',\n  'of',\n  'the',\n  'lord'],\n ['the',\n  'thorn',\n  'tree',\n  'cast',\n  'its',\n  'evil',\n  'fruit',\n  'and',\n  'blushed',\n  'with',\n  'plum',\n  'and',\n  'pear',\n  'and',\n  'seeded',\n  'grass',\n  'and',\n  'trodden',\n  'root',\n  'grew',\n  'sweet',\n  'beneath',\n  'their',\n 